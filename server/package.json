{
  "name": "financial-reporting-system",
  "version": "1.0.0",
  "description": "Backend App for financial reporting systems",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "server": "nodemon server.js",
    "dev": "nodemon server.js",
    "c:prettier": "npx prettier -c .",
    "w:prettier": "npx prettier --write .",
    "create-migrate": "npx sequelize migration:generate --name --config config/config.js",
    "migrate": "npx sequelize db:migrate --config config/config.js",
    "migrate-test": "npx sequelize db:migrate --env test --config config/config.js",
    "undo-migrate": "npx sequelize db:migrate:undo --config config/config.js",
    "undo-all-migrate": "npx sequelize db:migrate:undo:all --config config/config.js",
    "status-migrate": "npx sequelize db:migrate:status --config config/config.js",
    "create-seed": "npx sequelize-cli seed:generate --name --config config/config.js",
    "create-migrate-test": "$env:NODE_ENV='test'; npx sequelize-cli db:migrate --config config/config.js",
    "create-seed-test": "$env:NODE_ENV='test'; npx sequelize-cli db:seed:all --config config/config.js",
    "seed": "npx sequelize db:seed:all --config config/config.js",
    "seed-test": "npx sequelize db:seed:all --env test --config config/config.js"
  },
  "keywords": [],
  "author": "waqar ahmad",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.0",
    "express-validator": "^7.1.0",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mailtrap": "^3.4.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.10.1",
    "nodemailer": "^6.9.16",
    "nodemon": "^3.1.9",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.2"
  }
}
